{
    "crons": [
        { "path": "/api/streak/reset", "schedule": "5 21 * * *" },
        { "path": "/api/daily_quests_roll", "schedule": "0 21 * * *" }
    ],
    "headers": [{
        "source": "/(.*).svg",
        "headers": [
            { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
    }],
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/api/$1" },
        { "source": "/(.*)", "destination": "/index.html" }
    ]
}